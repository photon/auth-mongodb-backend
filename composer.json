{
    "name": "photon/auth-mongodb-backend",
    "type": "library",
    "description": "APIs for photon/auth-mongodb",
    "keywords": [
        "photon",
        "auth",
        "user",
        "mongodb",
        "api"
    ],
    "homepage": "http://www.photon-project.com/",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "William MARTIN",
            "email": "william.martin@power-lan.com",
            "role": "Developer"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/photon/auth-mongodb/blob/master/CONTRIBUTORS.md"
        }
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "/home/william/TRAVAIL/pl/photon/auth-mongodb"
      }
    ],
    "require": {
        "php" : "^7.0",
        "photon/auth-mongodb": "dev-master",
        "photon/api-json": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "photon\\auth\\api\\MongoDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpcs": "phpcs --standard=PSR2 src tests",
        "phpcbf": "phpcbf --standard=PSR2 src tests"
    }
}
